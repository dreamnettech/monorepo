{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "sourceMap": false,
    "strict": true,
    "noImplicitAny": false,
    "composite": true,
    "importHelpers": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  }
 }
