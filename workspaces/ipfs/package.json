{
  "name": "@opendreamnet/ipfs",
  "version": "2.2.0",
  "description": "Easily create and use an IPFS node, either in NodeJS or web browser.",
  "license": "MIT",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "author": {
    "name": "Ivan Bravo Bravo",
    "email": "kolessios@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamnettech/monorepo.git",
    "directory": "workspaces/app"
  },
  "scripts": {
    "lint": "eslint --fix --ext .ts,.js src",
    "test": "ts-mocha test/**/*.ts --recursive --exit",
    "clean": "shx rm -rf dist tsconfig.tsbuildinfo",
    "watch": "unbuild --stub",
    "build": "unbuild && webpack-cli",
    "release": "standard-version"
  },
  "engines": {
    "node": ">= 16"
  },
  "browser": {
    "fs": false,
    "fs-extra": false,
    "go-ipfs": false,
    "recursive-fs": false
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "standard-version": {
    "path": ".",
    "tag-prefix": "ipfs-v",
    "releaseCommitMessageFormat": "chore(release): ipfs@{{currentTag}}"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@fidian/dynamic-buffer": "^1.0.0",
    "@opendreamnet/app": "workspace:^1.1.3",
    "buffer": "^6.0.3",
    "crypto-key-composer": "^0.1.3",
    "fs-extra": "^10.1.0",
    "go-ipfs": "^0.12.2",
    "ipfs": "^0.62.3",
    "ipfs-http-client": "^56.0.3",
    "ipfsd-ctl": "^10.0.6",
    "it-all": "^1.0.6",
    "it-to-stream": "^1.0.0",
    "libp2p-crypto": "^0.21.2",
    "lodash": "^4.17.21",
    "mime": "^3.0.0",
    "node-abort-controller": "^3.0.1",
    "normalize-path": "^3.0.0",
    "peer-id": "^0.16.0",
    "process": "^0.11.10",
    "recursive-fs": "^2.1.0",
    "sanitize-filename": "^1.6.3",
    "speedometer": "^1.1.0",
    "stream-to-blob": "^2.0.1",
    "stream-to-blob-url": "^3.0.2",
    "urijs": "^1.19.11"
  },
  "devDependencies": {
    "@opendreamnet/build": "workspace:^",
    "@opendreamnet/eslint-config": "workspace:^",
    "@types/eslint": "^8",
    "@types/fs-extra": "9.0.13",
    "@types/lodash": "4.14.182",
    "@types/mime": "^2",
    "@types/normalize-path": "3.0.0",
    "@types/urijs": "^1",
    "@types/webpack-bundle-analyzer": "^4",
    "assert": "^2.0.0",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.14.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "shx": "^0.3.4",
    "standard-version": "^9.3.2",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "vm-browserify": "^1.1.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  }
}
