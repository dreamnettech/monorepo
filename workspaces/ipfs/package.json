{
  "name": "@opendreamnet/ipfs",
  "version": "0.1.6",
  "description": "Easily create and use an IPFS node in NodeJS and web browsers.",
  "license": "MIT",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.umd.js",
  "exports": {
    "browser": "./dist/index.umd.js",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint --fix --ext .ts,.js src",
    "test": "ts-mocha test/**/*.ts --recursive --exit",
    "clean": "shx rm -rf dist tsconfig.tsbuildinfo",
    "watch": "tsup-node --watch",
    "watch:browser": "webpack-cli watch",
    "build": "tsup-node && webpack-cli",
    "release": "standard-version",
    "ugly:test": "ts-node ./test/index.ts"
  },
  "author": {
    "name": "Ivan Bravo Bravo",
    "email": "ivan@opendreamnet.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamnettech/monorepo.git",
    "directory": "workspaces/ipfs"
  },
  "keywords": [
    "opendreamnet",
    "ipfs"
  ],
  "browser": {
    "fs": false,
    "fs-extra": false,
    "go-ipfs": false,
    "recursive-fs": false
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "standard-version": {
    "path": ".",
    "tag-prefix": "ipfs-v",
    "releaseCommitMessageFormat": "chore(release): ipfs@{{currentTag}}"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "@fidian/dynamic-buffer": "^1.0.0",
    "@opendreamnet/app": "workspace:^",
    "crypto-key-composer": "^0.1.3",
    "fs-extra": "^11.1.0",
    "ipfs": "^0.66.0",
    "ipfs-core-types": "^0.14.0",
    "ipfs-http-client": "^60.0.0",
    "ipfs-unixfs": "^9.0.1",
    "ipfsd-ctl": "^13.0.0",
    "it-all": "^1.0.6",
    "it-to-stream": "^1.0.0",
    "libp2p-crypto": "^0.21.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "luxon": "^3.3.0",
    "mime": "^3.0.0",
    "multiaddr": "^10.0.1",
    "multiformats": "^11.0.2",
    "node-abort-controller": "^3.1.1",
    "normalize-path": "^3.0.0",
    "peer-id": "^0.16.0",
    "recursive-fs": "^2.1.0",
    "sanitize-filename": "^1.6.3",
    "speedometer": "^1.1.0",
    "stream-to-blob": "^2.0.1",
    "stream-to-blob-url": "^3.0.2",
    "urijs": "^1.19.11"
  },
  "devDependencies": {
    "@opendreamnet/build": "workspace:^",
    "@opendreamnet/eslint-config": "workspace:^",
    "@rollup/plugin-typescript": "^11.0.0",
    "@skypack/package-check": "^0.2.2",
    "@swc/core": "^1.3.40",
    "@swc/helpers": "^0.4.14",
    "@types/eslint": "^8",
    "@types/fs-extra": "11.0.1",
    "@types/lodash": "4.14.191",
    "@types/lodash-es": "^4",
    "@types/luxon": "^3",
    "@types/mime": "^3",
    "@types/normalize-path": "3.0.0",
    "@types/urijs": "^1",
    "@types/webpack-bundle-analyzer": "^4",
    "assert": "^2.0.0",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.36.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "go-ipfs": "^0.18",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "regenerator-runtime": "^0.13.11",
    "rollup-plugin-ts": "^3.2.0",
    "shx": "^0.3.4",
    "standard-version": "^9.5.0",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.76.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1"
  },
  "optionalDependencies": {
    "go-ipfs": "^0.18"
  }
}
