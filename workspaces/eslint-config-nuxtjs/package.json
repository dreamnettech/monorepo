{
  "name": "@opendreamnet/eslint-config-nuxtjs",
  "packageManager": "yarn@3.2.0",
  "version": "1.5.3",
  "description": "ESLint config for OpenDreamNet projects in NuxtJS.",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "release": "standard-version"
  },
  "author": {
    "name": "Iv√°n Bravo Bravo",
    "email": "kolessios@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamnettech/monorepo.git",
    "directory": "workspaces/eslint-config-nuxtjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "standard-version": {
    "path": ".",
    "tag-prefix": "eslint-config-nuxtjs-v",
    "releaseCommitMessageFormat": "chore(release): eslint-config-nuxtjs@{{currentTag}}"
  },
  "files": [
    "index.js",
    ".eslintrc.js"
  ],
  "dependencies": {
    "@nuxtjs/eslint-config": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@opendreamnet/eslint-config": "workspace:^",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-vue": "^8.7.1"
  },
  "devDependencies": {
    "@types/eslint": "8.4.2",
    "eslint": "^8.14.0",
    "standard-version": "^9.3.2"
  },
  "peerDependencies": {
    "eslint": "^8"
  }
}
