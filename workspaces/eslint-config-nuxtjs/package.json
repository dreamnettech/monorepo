{
  "name": "@opendreamnet/eslint-config-nuxtjs",
  "packageManager": "yarn@3.2.0",
  "version": "0.1.0",
  "description": "ESLint config for OpenDreamnet projects.",
  "license": "MIT",
  "main": ".eslintrc.js",
  "scripts": {
    "release": "standard-version"
  },
  "author": {
    "name": "Iv√°n Bravo Bravo",
    "email": "ivan@opendreamnet.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "standard-version": {
    "path": ".",
    "tag-prefix": "eslint-config-nuxtjs-v",
    "releaseCommitMessageFormat": "chore(release): eslint-config-nuxtjs@{{currentTag}}"
  },
  "files": [
    ".eslintrc.js"
  ],
  "dependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@opendreamnet/eslint-config": "workspace:^",
    "@rushstack/eslint-patch": "^1.5.1"
  },
  "devDependencies": {
    "@types/eslint": "8.44.6",
    "eslint": "^8.52.0",
    "standard-version": "^9.5.0"
  },
  "peerDependencies": {
    "eslint": "^8.52.0"
  }
}
