{
  "name": "@opendreamnet/nuxtjs-base",
  "version": "1.6.1",
  "license": "MIT",
  "main": "nuxt.config.js",
  "scripts": {
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "path": ".",
    "tag-prefix": "nuxtjs-base-v",
    "releaseCommitMessageFormat": "chore(release): nuxtjs-base@{{currentTag}}"
  },
  "author": {
    "name": "Ivan Bravo Bravo",
    "email": "kolessios@gmail.com",
    "url": "https://github.com/dreamnettech/monorepo/graphs/contributors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamnettech/monorepo.git",
    "directory": "workspaces/app"
  },
  "bugs": {
    "email": "kolessios@gmail.com",
    "url": "https://github.com/dreamnettech/monorepo/issues"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "tinycolor2": "^1.4.2",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@opendreamnet/eslint-config-nuxtjs": "workspace:^",
    "@types/lodash": "4.14.182",
    "@types/tinycolor2": "^1",
    "eslint": "^7.29.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4"
  },
  "peerDependencies": {
    "lodash": "^4",
    "tippy.js": "^6",
    "@nuxtjs/style-resources": "*",
    "@nuxtjs/markdownit": "*",
    "@nuxtjs/tailwindcss": "*"
  }
}
