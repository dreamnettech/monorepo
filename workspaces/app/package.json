{
  "name": "@opendreamnet/app",
  "version": "1.1.3",
  "description": "Utilities for OpenDreamNet applications.",
  "license": "MIT",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "author": {
    "name": "Ivan Bravo Bravo",
    "email": "kolessios@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamnettech/monorepo.git",
    "directory": "workspaces/app"
  },
  "scripts": {
    "lint": "eslint --fix --ext .ts,.js src",
    "test": "ts-mocha test/**/*.ts --recursive --exit",
    "clean": "shx rm -rf tsconfig.tsbuildinfo dist",
    "watch": "unbuild --stub",
    "build": "unbuild && webpack-cli",
    "release": "standard-version"
  },
  "engines": {
    "node": ">= 16"
  },
  "browser": {
    "electron": false,
    "fs": false,
    "os": false,
    "parent-package-json": false,
    "path": false,
    "platform-folders": false
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "standard-version": {
    "path": ".",
    "tag-prefix": "app-v",
    "releaseCommitMessageFormat": "chore(release): app@{{currentTag}}"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "lodash": "^4.17.21",
    "parent-package-json": "^2.0.1"
  },
  "devDependencies": {
    "@opendreamnet/build": "workspace:^",
    "@opendreamnet/eslint-config": "workspace:^",
    "@types/eslint": "8.4.2",
    "@types/lodash": "4.14.182",
    "@types/selenium-webdriver": "4.0.19",
    "eslint": "^8.14.0",
    "platform-folders": "^0.6",
    "selenium-webdriver": "^4.1.2",
    "shx": "^0.3.4",
    "standard-version": "^9.3.2",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "webpack-cli": "^4.9.2"
  },
  "peerDependencies": {
    "platform-folders": "^0.6"
  }
}
