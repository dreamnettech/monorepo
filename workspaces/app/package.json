{
  "name": "@opendreamnet/app",
  "version": "1.1.3",
  "description": "Utilities for DreamNet applications.",
  "license": "MIT",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "unpkg": "dist/app.umd.js",
  "scripts": {
    "lint": "eslint --fix --ext .ts,.js src",
    "test": "ts-mocha test/**/*.ts --recursive --exit",
    "clean": "shx rm -rf tsconfig.tsbuildinfo dist",
    "watch": "tsc -w",
    "build:web": "webpack",
    "build:node": "tsc -b",
    "build": "yarn run build:node && yarn run build:web",
    "release": "yarn dlx standard-version && git push --follow-tags"
  },
  "browser": {
    "electron": false,
    "fs": false,
    "os": false,
    "parent-package-json": false,
    "path": false,
    "platform-folders": false
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "parent-package-json": "^2.0.1",
    "platform-folders": "^0.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@dreamnet/eslint-config-dreamnet": "workspace:workspaces/eslint-config-dreamnet",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/eslint": "7.2.13",
    "@types/lodash": "4.14.170",
    "@types/selenium-webdriver": "4.0.13",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-imports": "^2.0.0",
    "eslint": "^7.28.0",
    "path-browserify": "^1.0.1",
    "selenium-webdriver": "^4.0.0-beta.4",
    "shx": "^0.3.3",
    "ts-loader": "^9.2.3",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^3.9.9",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
  },
  "standard-version": {
    "path": ".",
    "tag-prefix": "app-v",
    "releaseCommitMessageFormat": "chore(release): app@{{currentTag}}"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "author": {
    "name": "Ivan Bravo Bravo",
    "email": "kolessios@gmail.com",
    "url": "https://github.com/dreamnettech/monorepo/graphs/contributors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamnettech/monorepo.git",
    "directory": "workspaces/app"
  },
  "bugs": {
    "email": "ivan@dreamnet.tech",
    "url": "https://github.com/dreamnettech/monorepo/issues"
  },
  "engines": {
    "node": ">= 14"
  }
}
