  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/app.js",
  "exports": "dist/app.modern.js",
  "unpkg": "dist/app.umd.js",
  "scripts": {
    "prepack": "yarn run build",
    "lint": "eslint --fix --ext .ts,.js src",
    "test": "ts-mocha test/**/*.ts --recursive --exit",
    "clean": "shx rm -rf dist",
    "watch": "microbundle watch -f cjs",
    "build:web": "microbundle -f modern,umd --external none",
    "build:node": "microbundle -f cjs --target node --compress",
    "build": "yarn run build:node && yarn run build:web"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "parent-package-json": "^2.0.1",
    "platform-folders": "^0.5.1"
