    "@types/lodash": "4.14.151",
    "@types/selenium-webdriver": "4.0.9",
    "chromedriver": "^85.0.1",
    "eslint": "^7.0.0",
    "microbundle": "^0.12.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "shx": "^0.3.2",
    "ts-mocha": "^7.0.0",
    "typescript": "^3.9.9"
  },
