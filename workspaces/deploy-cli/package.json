{
  "name": "@opendreamnet/deploy-cli",
  "description": "CLI utility for @opendreamnet/deploy",
  "version": "1.3.1",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --fix --ext .ts,.js src",
    "clean": "shx rm -rf tsconfig.tsbuildinfo dist",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "watch": "tsc -w",
    "build": "tsc -b",
    "release": "standard-version"
  },
  "bin": {
    "deploy": "./bin/run"
  },
  "oclif": {
    "bin": "deploy"
  },
  "author": {
    "name": "Ivan Bravo Bravo",
    "email": "kolessios@gmail.com",
    "url": "https://github.com/dreamnettech/monorepo/graphs/contributors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamnettech/monorepo.git",
    "directory": "workspaces/deploy-cli"
  },
  "bugs": {
    "email": "kolessios@gmail.com",
    "url": "https://github.com/dreamnettech/monorepo/issues"
  },
  "files": [
    "/bin",
    "/dist"
  ],
  "standard-version": {
    "path": ".",
    "tag-prefix": "deploy-cli-v",
    "releaseCommitMessageFormat": "chore(release): deploy-cli@{{currentTag}}",
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "improvement",
        "section": "Improvements"
      },
      {
        "type": "docs",
        "section": "Miscellaneous changes"
      },
      {
        "type": "style",
        "section": "Design"
      },
      {
        "type": "refactor",
        "section": "Miscellaneous changes"
      },
      {
        "type": "perf",
        "section": "Improvements"
      },
      {
        "type": "test",
        "hidden": true
      },
      {
        "type": "chore",
        "section": "Miscellaneous changes"
      }
    ]
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@opendreamnet/deploy": "workspace:^1.4.0",
    "lodash": "^4.17.21",
    "ora": "^5.4.1",
    "tslib": "^2"
  },
  "devDependencies": {
    "@dreamnet/eslint-config-dreamnet": "workspace:workspaces/eslint-config-dreamnet",
    "@oclif/dev-cli": "^1.26.0",
    "eslint": "^7.31.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "shx": "^0.3.3",
    "standard-version": "^9.3.1",
    "typescript": "^3.9.9"
  }
}
