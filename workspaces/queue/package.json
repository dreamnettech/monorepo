{
  "name": "@dreamnet/queue",
  "version": "1.1.2",
  "description": "Small library to create managed queues.",
  "license": "MIT",
  "source": "src/queue.ts",
  "types": "dist/queue.d.ts",
  "main": "dist/queue.js",
  "unpkg": "dist/queue.umd.js",
  "scripts": {
    "lint": "eslint --fix --ext .ts,.js src",
    "test": "ts-mocha test/**/*.ts --recursive --exit",
    "clean": "shx rm -rf dist",
    "watch": "tsc -w",
    "build:web": "webpack",
    "build:node": "tsc -b",
    "build": "yarn run build:node && yarn run build:web",
    "release": "yarn dlx standard-version && git push --follow-tags"
  },
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@dreamnet/eslint-config-dreamnet": "workspace:^1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/mocha": "^8",
    "@types/source-map-support": "^0",
    "eslint": "^7.18.0",
    "husky": "^7.0.1",
    "lint-staged": "^10.5.3",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "shx": "^0.3.3",
    "source-map-support": "^0.5.19",
    "ts-loader": "^8.1.0",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.7",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0"
  },
  "standard-version": {
    "path": ".",
    "tag-prefix": "queue-v",
    "releaseCommitMessageFormat": "chore(release): queue@{{currentTag}}"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "author": {
    "name": "Ivan Bravo Bravo",
    "email": "kolessios@gmail.com",
    "url": "https://github.com/dreamnettech/monorepo/graphs/contributors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamnettech/monorepo.git",
    "directory": "workspaces/queue"
  },
  "bugs": {
    "email": "ivan@dreamnet.tech",
    "url": "https://github.com/dreamnettech/monorepo/issues"
  },
  "engines": {
    "node": ">= 14"
  }
}
