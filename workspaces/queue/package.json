{
  "name": "@dreamnet/queue",
  "version": "1.1.0",
  "description": "Small library to create managed queues.",
  "license": "MIT",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "prepack": "yarn run build",
    "lint": "eslint --fix --ext .ts,.js src",
    "test": "ts-mocha test/**/*.ts --recursive --exit",
    "clean": "shx rm -rf dist",
    "watch": "microbundle watch -f cjs",
    "build:web": "microbundle -f modern,umd --external none --target node --compress",
    "build:node": "tsc -b",
    "build": "yarn run build:node && yarn run build:web"
  },
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@dreamnet/eslint-config-dreamnet": "workspace:^1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/source-map-support": "^0",
    "eslint": "^7.18.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "microbundle": "^0.12.4",
    "nyc": "^15.1.0",
    "shx": "^0.3.3",
    "source-map-support": "^0.5.19",
    "ts-mocha": "^8.0.0",
    "typescript": "^3.9.7"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "author": {
    "name": "Contributors",
    "email": "ivan@dreamnet.tech",
    "url": "https://github.com/dreamnettech/monorepo/graphs/contributors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamnettech/monorepo.git",
    "directory": "workspaces/queue"
  },
  "bugs": {
    "email": "ivan@dreamnet.tech",
    "url": "https://github.com/dreamnettech/monorepo/issues"
  },
  "engines": {
    "node": ">= 12"
  }
}
