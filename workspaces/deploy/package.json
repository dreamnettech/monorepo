{
  "name": "@opendreamnet/deploy",
  "description": "Deploy files and folders to different file providers.",
  "version": "1.4.1",
  "license": "MIT",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --fix --ext .ts,.js src",
    "clean": "shx rm -rf tsconfig.tsbuildinfo dist",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "watch": "tsc -w",
    "build": "tsc -b",
    "release": "standard-version"
  },
  "author": {
    "name": "IvÃ¡n Bravo Bravo",
    "email": "kolessios@gmail.com",
    "url": "https://github.com/dreamnettech/monorepo/graphs/contributors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamnettech/monorepo.git",
    "directory": "workspaces/deploy"
  },
  "bugs": {
    "email": "kolessios@gmail.com",
    "url": "https://github.com/dreamnettech/monorepo/issues"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "standard-version": {
    "path": ".",
    "tag-prefix": "deploy-v",
    "releaseCommitMessageFormat": "chore(release): deploy@{{currentTag}}",
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "improvement",
        "section": "Improvements"
      },
      {
        "type": "docs",
        "section": "Miscellaneous changes"
      },
      {
        "type": "style",
        "section": "Design"
      },
      {
        "type": "refactor",
        "section": "Miscellaneous changes"
      },
      {
        "type": "perf",
        "section": "Improvements"
      },
      {
        "type": "test",
        "hidden": true
      },
      {
        "type": "chore",
        "section": "Miscellaneous changes"
      }
    ]
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@octokit/rest": "^18.7.0",
    "@opendreamnet/app": "workspace:workspaces/app",
    "axios": "^0.21.1",
    "cids": "^1.1.7",
    "cryptr": "^6.0.2",
    "dotenv": "^10.0.0",
