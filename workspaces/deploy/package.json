    "mime-types": "^2.1.31",
    "minio": "^7.0.18",
    "multiaddr": "^10.0.0",
    "normalize-path": "^3.0.0",
    "ora": "^5.4.1",
    "recursive-fs": "^2.1.0",
    "slash": "^3.0.0",
    "web3.storage": "^3.3.0"
  },
  "devDependencies": {
    "@opendreamnet/eslint-config": "workspace:^",
    "@types/chai": "^4",
    "@types/cryptr": "4.0.1",
    "@types/fs-extra": "9.0.12",
    "@types/lodash": "4.14.171",
    "@types/megajs": "0.14.1",
    "@types/mime-types": "^2.1.0",
    "@types/minio": "^7.0.8",
