    "eslint": "^7.31.0",
    "mocha": "^9",
    "nyc": "^15.1.0",
    "shx": "^0.3.3",
    "standard-version": "^9.3.1",
    "ts-node": "^10.1.0",
    "typescript": "^3.9.9"
  }
